		"@rollup/plugin-commonjs": "^25.0.5",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-typescript": "^11.1.5",
		"@types/react": "^18.2.28",
		"@types/rollup-plugin-peer-deps-external": "^2.2.3",
		"rollup": "^4.0.2",
		"rollup-plugin-dts": "^6.1.0",
