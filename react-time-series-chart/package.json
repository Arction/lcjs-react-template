{
	"name": "@arction/react-time-series-chart",
	"author": "LightningChart Ltd.",
	"license": "MIT",
	"version": "0.0.1",
	"description": "A React component for interactive, heavy-duty Time Series Charts",
	"homepage": "https://github.com/Arction/lcjs-react-template/react-time-series-chart",
	"type": "module",
	"main": "dist/cjs/index.js",
	"module": "dist/esm/index.js",
	"types": "dist/types/index.d.ts",
	"private": false,
	"engines": {
		"node": ">=12"
	},
	"files": [
		"dist",
		"LICENSE",
		"README.md",
		"package.json"
	],
	"scripts": {
		"build": "rm -r dist & rollup -c --bundleConfigAsCjs",
